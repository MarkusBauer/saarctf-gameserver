{
  "name": "scoreboard",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "environment": "if [ -z \"$SAARCTF_ENVIRONMENT\" ]; then ../run.sh saarctf_commons/config.py get environment || echo None ; else echo $SAARCTF_ENVIRONMENT ; fi",
    "environment_config": "e=$(npm run -s environment) ; if [ \"$e\" = \"intern\" ]; then echo ,intern ; elif [ \"$e\" = \"dev\" ] ; then echo ,dev ; fi",
    "build": "ng build -c \"production$(npm run -s environment_config)\" --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.12",
    "@angular/cdk": "^20.2.13",
    "@angular/common": "^20.3.12",
    "@angular/compiler": "^20.3.12",
    "@angular/core": "^20.3.12",
    "@angular/forms": "^20.3.12",
    "@angular/platform-browser": "^20.3.12",
    "@angular/platform-browser-dynamic": "^20.3.12",
    "@angular/router": "^20.3.12",
    "@efaps/ngx-store": "^11.0.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@kurkle/color": "^0.4.0",
    "bootstrap": "^3.4.1",
    "chart.js": "^4.5.1",
    "lodash": "^4.17.21",
    "ng2-charts": "^8.0.0",
    "ngx-bootstrap": "^20.0.2",
    "rxjs": "~7.8.1",
    "ts-debug": "^1.3.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.10",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.12",
    "@angular/language-service": "^20.3.12",
    "@types/node": "^12.11.1",
    "@types/lodash": "^4.17.13",
    "gzip-all": "^1.0.0",
    "less": "^4.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~5.8.3"
  }
}
